{
  "name": "xngllamasearch-dev",
  "image": "mrhappynice/bplus-xngllamasearch",
  "runArgs": [
    "--network=host",
    "--device=/dev/dri",
    "--group-add=video"
  ],
  "mounts": [
    "source=${localWorkspaceFolder}/config,target=/etc/searxng,type=bind",
    "source=${localWorkspaceFolder}/data,target=/var/cache/searxng,type=bind"
  ],
  "containerEnv": {
    "INSTANCE_NAME": "SearXNG",
    "BASE_URL": "http://localhost:8080/",
    "SEARXNG_URL": "http://127.0.0.1:8080",
    "AUTH_USERNAME": "",
    "AUTH_PASSWORD": "",
    "OPENAI_API_BASE": "http://localhost:1234/v1",
    "LMSTUDIO_API_BASE": "http://localhost:1234/v1",
    "OPENAI_API_KEY": "sk-proj-xxxxx-x-x-x-hwA",
    "OPENROUTER_API_KEY": "sk-oxxxxr-xx-xxxx",
    "GOOGLE_API_KEY": "xxxxx",
    "MODEL": "bartowski/Llama-3.2-1B-Instruct-GGUF:Q4_K_M",
    "USE_NATIVE": "1"
  },
  "remoteUser": "root",
  "forwardPorts": [8080],
  "postCreateCommand": "echo 'âœ… Loaded - connect to launcher http://localhost:5660'"
}
